{{yield}}

{{#each feedItems as |feedItem index|}}
  {{feed/feed-item
    model=feedItem
    isSearchActive=isSearchActive
    canEditIfAllowed=canEditIfAllowed
    showAnyViewCount=showAnyViewCount
    enableImpressionTracking=enableImpressionTracking
    impressionLocation="index-feed"
  }}

  {{#if (show-feed-ad index)}}
    <div class="AdBanner-wrapper AdBanner-wrapper--feed">
      {{ad-banner
        adContextName="feed"
        impressionPath="feed"
        pagePositionForAnalytics=index
        placeholderBlockFixedSize=true
      }}
    </div>
  {{/if}}
{{/each}}
