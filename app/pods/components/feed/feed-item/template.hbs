{{#if isContent}}
  {{feed-card
    model=feedItemModel
    organization=organization
    allowManageOnTile=allowManageOnTile
    displayAsPublic=displayAsPublic
    condensedView=condensedView
    impressionsEnabled=enableImpressionTracking
    impressionLocation=impressionLocation
  }}
{{else if isCarousel}}
  {{feed-carousel
    model=feedItemModel
    isSearchActive=isSearchActive
    enableImpressionTracking=enableImpressionTracking
  }}
{{else if isOrganization}}
  {{#x-card}}
    {{cards/organization-profile-header
      organization=feedItemModel
      showGearButton=false
    }}
    {{cards/organization-contact
      organization=feedItemModel
    }}
  {{/x-card}}
{{/if}}
