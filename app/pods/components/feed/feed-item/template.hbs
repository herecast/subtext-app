{{#if isContent}}
  {{feed-card
    model=feedItemModel
    impressionsEnabled=enableImpressionTracking
    impressionLocation=impressionLocation
    showAnyViewCount=showAnyViewCount
  }}
{{else if isCarousel}}
  {{feed-carousel
    model=feedItemModel
    isSearchActive=isSearchActive
    enableImpressionTracking=enableImpressionTracking
  }}
{{/if}}
