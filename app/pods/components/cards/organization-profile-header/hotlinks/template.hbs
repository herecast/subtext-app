{{#divided-list}}
  {{#each customLinks as |customLink|}}
    <li>
      {{#if customLink.eventInstanceId}}
        {{#x-link-to  "profile.all.show-instance"
                       organization.organizationLinkId
                       customLink.contentId
                       customLink.eventInstanceId
                       color="primary"
                       size="medium"}}
          {{customLink.title}}
        {{/x-link-to}}
      {{else}}
        {{#x-link-to "profile.all.show"
                     organization.organizationLinkId
                     customLink.contentId
                     color="primary"
                     size="medium"}}
          {{customLink.title}}
        {{/x-link-to}}
      {{/if}}
    </li>

  {{/each}}
{{/divided-list}}
