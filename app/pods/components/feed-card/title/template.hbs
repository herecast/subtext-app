{{#if showToggleSold}}
  <div class="FeedCard-Title-sold-toggle" {{action "toggleSold"}} role="button">
    {{#if sold}}
      {{fa-icon "toggle-on" class="toggle-on"}}
      <div>Sold</div>
    {{else}}
      {{fa-icon "toggle-off"}}
      <div>Not Sold</div>
    {{/if}}
  </div>
{{else if sold}}
  <div class="FeedCard-Title-sold-tag">
   Sold
  </div>
{{/if}}

<div class="FeedCard-Title-title title-real" {{action onContentClick}} role="button" data-test-card-title>
  {{#feed-card/conditional-link-to
    useLink=linkToDetailIsActive
    model=model
  }}
    {{title}}

  {{/feed-card/conditional-link-to}}
</div>

<div class="FeedCard-Title--menu">
  {{feed-card/options-menu
    model=model
    isOnDetailView=isOnDetailView
  }}
</div>

<div class="FeedCard-Title-title title-clone">
  {{title}}
</div>
