<div class="FeedCard--card-front">
  {{feed-card/title
    title=model.title
    model=model
  }}

  {{#if model.primaryImageUrl}}
    {{feed-card/image
      imageUrl=model.primaryImageUrl
      model=model
    }}
  {{else}}
    {{feed-card/text-snippet
      model=model
      showTextSnippet=true
    }}
  {{/if}}

  {{feed-card/attribution
    avatarUrl=model.attributionImageUrl
    author=model.attributionName
    linkRouteName=attributionLinkRouteName
    linkId=attributionLinkId
    postedTime=model.publishedAtRelative
    isListserv=model.isListserv
    commentCount=model.commentCount
    contentId=model.contentId
  }}

  {{feed-card/footer
    locationTagName=sourceTag
    commentCount=model.commentCount
  }}
</div>
