{{#if model.logoUrl}}
  <img src={{model.logoUrl}} data-test-organization-image class='OrganizationAbout-avatar'>
{{/if}}
{{#if model.description}}
<h1 class='OrganizationAbout-title'>
  About
  {{model.name}}
</h1>
<p class='OrganizationAbout-description' data-test-organization-description>
  {{descriptionHtml}}
</p>
{{/if}}