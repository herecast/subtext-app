{{#modal-wrapper
  close=closeDetailPage
  showHeader=false
  slideInFrom=(if isMobile "right" null)
  slideOutTo=(if isMobile "right" null)
  slideMessage=(if isMobile slideMessage null)
  revealNav=true
  class="Modal--no-padding"
  isDetailPage=true
  willAnimateAway=isMobile
}}

  {{detail-page
    model=model
    closeRoute="feed"
    isPreview=false
    enableStickyHeader=true
    onClose=closeDetailPage
    trackDetailEngagement=trackDetailEngagement
  }}

{{/modal-wrapper}}
