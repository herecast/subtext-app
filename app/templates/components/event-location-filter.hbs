{{input
  placeholder='Type to search'
  class='Dropdown-input FilterBar-input FilterBar-inputLocation dropdown-toggle'
  value=inputValue
}}

<ul class='dropdown-menu' role='menu'>
  {{#each place in places}}
    <li><a href='#' {{action 'setLocation' place}} tabindex='-1'>{{place}}</a></li>
  {{else}}
    <li class='disabled'><a tabindex='-1'>No Results</a></li>
  {{/each}}
</ul>
